          ),
       table_3
       AS (SELECT bounced,
                  contact_us_entry,
                  contact_us_submitted,
                  device_type,
                  email_subscription_entry,
                  email_subscription_submitted,
                  hard_bounce,
                  landing_search_engine,
                  last_touch_campaign,
                  new_visid,
